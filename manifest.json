{
  "name": "Test Folkeregister",
  "short_name": "TFR",
  "description": "Extension versjon av testfolkeregister.",
  "version": "1.2.2",
  "permissions": ["declarativeContent","contextMenus","activeTab","<all_urls>","tabs","storage","input","debugger"],
  "background": {
    "scripts": ["sample.js"]
  },
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "contentScript.js"
        ]
    }
],
"browser_action": {
  "default_title":"Test Folkeregister",
  "default_popup": "popup.html",
  "default_icon": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
},
"icons": {
  "16": "images/icon16.png",
  "32": "images/icon32.png",
  "48": "images/icon48.png",
  "128": "images/icon128.png"
},
  "manifest_version": 2
}
